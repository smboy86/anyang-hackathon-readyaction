{
  "expo": {
    "name": "레디액션",
    "slug": "readyaction",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_app_1.png",
